datasource db {
  provider = "postgresql"
  url      = env("PRISMA_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                 Int                @id @unique @default(autoincrement())
  email              String             @unique @db.VarChar(255)
  username           String             @unique @db.VarChar(63)
  password           String
}
